library("QTLseqr")
library("dplyr")
HighBulk <- "group-3-LF"
LowBulk <- "group-1-EF"
file <- "SNPs_from_GATK-EF-LF.table"
Chroms <- paste0(rep("C", 9), 1:9)
df <- importFromGATK(
file = file,
highBulk = HighBulk,
lowBulk = LowBulk,
chromList = Chroms
)
df_filt <- filterSNPs(
SNPset = df,
refAlleleFreq = 0.20,
minTotalDepth = 100,
maxTotalDepth = 400,
minSampleDepth = 40,
minGQ = 99
)
df_filt <- runGprimeAnalysis(
SNPset = df_filt,
windowSize = 1e6,
outlierFilter = "deltaSNP"
)
snps <- filter(df_filt, Gprime > 2.5)
write.csv(snps, file="SNPs-gprime2.5-EF-LF.csv")
View(snps)
